R?(x text).

dd_delta_R?(x text).

dd_new_R?(x text).

dd_new_R(x) :-
    R(x);
    dd_delta_R(x).

S(x text).

dd_delta_S(x text).

dd_new_S(x text).

dd_new_S(x) :-
    S(x);
    dd_delta_S(x).

T(x text,
  f text).

dd_delta_T(x text,
           f text).

dd_new_T(x text,
         f text).

dd_new_T(x, f) :-
    T(x, f);
    dd_delta_T(x, f).

U(x text,
  l text).

dd_delta_U(x text,
           l text).

dd_new_U(x text,
         l text).

dd_new_U(x, l) :-
    U(x, l);
    dd_delta_U(x, l).

dd_delta_R(x) :-
    dd_delta_S(x),
    T(x, f),
    U(x, l);
    dd_new_S(x),
    dd_delta_T(x, f),
    U(x, l);
    dd_new_S(x),
    dd_new_T(x, f),
    dd_delta_U(x, l).

